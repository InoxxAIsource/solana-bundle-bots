# Solana Network Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_WEBSOCKET_URL=wss://api.devnet.solana.com
NETWORK=devnet

# Wallet Configuration
MASTER_WALLET_PATH=./keys/master-wallet.json
WALLET_CONFIG_PATH=./keys/wallet-config.json
ENCRYPTION_PASSWORD=change-this-to-a-secure-password

# Bundle Program Configuration
BUNDLE_PROGRAM_ID=YourDeployedProgramId

# Server Configuration
PORT=3001
FRONTEND_URL=http://localhost:3000

# Security Configuration
JWT_SECRET=change-this-to-a-secure-jwt-secret
API_RATE_LIMIT=100
# MEV Protection Configuration
PRIVATE_RPC_ENABLED=false
PRIVATE_RPC_URL=
OBFUSCATION_ENABLED=true
PRIORITY_FEE_MULTIPLIER=1.5
HIDDEN_ROUTING_ENABLED=true
TIME_RANDOMIZATION_ENABLED=true

# Arbitrage Configuration
MIN_PROFIT_PERCENTAGE=0.5
MAX_SLIPPAGE=1.0
GAS_COST_ESTIMATE_SOL=0.001
ARBITRAGE_MONITORING_INTERVAL=30000

# Copy Trading Configuration
COPY_MONITORING_INTERVAL=10000
COPY_EXECUTION_INTERVAL=5000
COPY_DEFAULT_DELAY=5000
COPY_DEFAULT_MULTIPLIER=1.0
COPY_MAX_WALLETS=10
